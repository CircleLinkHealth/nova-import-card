{
    "name": "cpm-web",
    "private": true,
    "scripts": {
        "build-cpm": "COMPOSER_MEMORY_LIMIT=-1 composer install && php artisan migrate && php artisan migrate:views && npm install && npm run bower_install",
        "build-dev": "npm run build-cpm && npm run dev && COMPOSER_MEMORY_LIMIT=-1 composer ide && sh ./scripts/install",
        "build-prod": "npm run build-cpm && npm run prod",
        "dev": "npm run development",
        "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch": "npm run development -- --watch",
        "watch-poll": "npm run watch -- --watch-poll",
        "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
        "prod": "npm run production",
        "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "test": "jest --verbose --coverage",
        "watch-test": "npm run test -- --watch",
        "bower_install": "./node_modules/bower/bin/bower -V install",
        "build-import-practice-staff-csv": "cd nova-components/ImportPracticeStaffCsv && npm run dev",
        "build-import-practice-staff-csv-prod": "cd nova-components/ImportPracticeStaffCsv  && npm install && npm run prod",
        "postinstall": "npm run bower_install && if [ \"$NODE_ENV\" = \"production\" ]; then npm run prod; else npm run dev; fi",
        "heroku-postbuild": "rm -rf node_modules/.cache",
        "build-generate-patient-call-data-csv": "cd nova-components/GeneratePatientCallDataCsv && npm run dev",
        "build-generate-patient-call-data-csv-prod": "cd nova-components/GeneratePatientCallDataCsv && npm install && npm run prod",
        "build-clhImportCardExtended": "cd nova-components/ClhImportCardExtended && npm run dev",
        "build-clhImportCardExtended-prod": "cd nova-components/ClhImportCardExtended && npm install && npm run prod",
        "build-generate-success-stories-report": "cd nova-components/GenerateSuccessStoriesReport && npm run dev",
        "build-generate-success-stories-report-prod": "cd nova-components/GenerateSuccessStoriesReport && npm install && npm run prod",
        "build-enrollment-invites": "cd nova-components/EnrollmentInvites && npm run dev",
        "build-enrollment-invites-prod": "cd nova-components/EnrollmentInvites && npm install && npm run prod",
        "build-invoices-download": "cd nova-components/InvoicesDownload && npm run dev",
        "build-invoices-download-prod": "cd nova-components/InvoicesDownload && npm run prod"
    },
    "jest": {
        "verbose": true,
        "moduleNameMapper": {
            "^vue$": "vue/dist/vue.common.js"
        },
        "moduleFileExtensions": [
            "js",
            "vue"
        ],
        "transform": {
            "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
            ".*\\.(vue)$": "<rootDir>/node_modules/jest-vue-preprocessor"
        },
        "setupFiles": [
            "<rootDir>/resources/assets/js/tests/index.js"
        ],
        "mapCoverage": true
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^1.2.30",
        "@fortawesome/free-solid-svg-icons": "^5.14.0",
        "@fortawesome/vue-fontawesome": "^0.1.10",
        "@fullcalendar/core": "^4.4.2",
        "@types/mysql": "^2.15.15",
        "@types/node": "^12.12.52",
        "autocomplete-vue": "^1.0.3",
        "axios": "^0.19",
        "bootstrap": "^4.5.0",
        "bower": "^1.8.2",
        "cross-env": "^7.0",
        "ejs": "^3.1.3",
        "es6-string-polyfills": "^1.0.0",
        "jquery": "^3.5.1",
        "laravel-echo": "^1.8.0",
        "laravel-mix": "^5.0.1",
        "less": "^2.7.2",
        "less-loader": "^4.0.5",
        "lodash": "^4.17.19",
        "logdna": "^3.5.1",
        "material-design-lite": "^1.3.0",
        "materialize-css": "^0.99.0",
        "minimist": "^1.2.5",
        "moment": "^2.27.0",
        "mysql": "^2.18.1",
        "node-excel-export": "github:CircleLinkHealth/node-excel-export#development",
        "pkg": "^4.4.9",
        "popper.js": "^1.16.1",
        "pusher-js": "^4.4.0",
        "rrule": "^2.6.4",
        "select2": "^4.0.13",
        "twilio-client": "^1.12.1",
        "v-autocomplete": "^1.8.1",
        "v-complete": "github:CircleLinkHealth/v-complete",
        "velocity-animate": "^1.5.0",
        "vue": "^2.5.17",
        "vue-axios": "^2.0.2",
        "vue-bootstrap-toggle": "^1.1.4",
        "vue-form": "^4.4.1",
        "vue-full-calendar": "^2.8.0",
        "vue-mdl": "^1.1.1",
        "vue-multiselect": "^2.1.6",
        "vue-select": "^2.5.1",
        "vue-simple-suggest": "^1.10.1",
        "vue-tables-2": "^0.6.99",
        "vue-touch-keyboard": "^0.3.0",
        "vue-trix": "^1.1.11",
        "vue2-dropzone": "^2.3.6",
        "vuejs-datepicker": "^1.6.2",
        "vuejs-paginator": "^2.0.2",
        "vuex": "^2.4.0",
        "webpack": "^4.44.0",
        "xmldom": "^0.1.31"
    },
    "devDependencies": {
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@babel/preset-env": "^7.10.4",
        "axios-mock-adapter": "^1.18.2",
        "babel-jest": "^21.2.0",
        "eslint": "^5.5.0",
        "jest": "^21.2.1",
        "jest-vue-preprocessor": "^1.7.1",
        "jshint": "^2.11.1",
        "laravel-profiler-client": "^1.3.0",
        "mock-local-storage": "^1.1.15",
        "resolve-url-loader": "^3.1.0",
        "sass": "^1.26.10",
        "sass-loader": "^8.0.0",
        "vue-template-compiler": "^2.6.6",
        "vue-test-utils": "^1.0.0-beta.4",
        "workbox-webpack-plugin": "^2.1.0",
        "xlsx": "^0.11.12",
        "xlsx-style": "^0.8.13"
    },
    "engines": {
        "node": "12.13.1",
        "npm": "6.12.1"
    },
    "license": "MIT"
}
