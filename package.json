{
    "name": "awv",
    "private": true,
    "scripts": {
        "build-awv": "composer install && php artisan migrate && php artisan migrate:views && npm install",
        "build-dev": "npm run build-awv && npm run dev && composer ide",
        "build-prod": "npm run build-awv && npm run prod",
        "dev": "npm run development",
        "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
        "prod": "npm run production",
        "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "start": "vue-cli-service serve --open",
        "watch": "npm run development -- --watch",
        "watch-poll": "npm run watch -- --watch-poll",
        "postinstall": "if [ \"$NODE_ENV\" = \"production\" ]; then npm run prod; else npm run dev; fi"
    },
    "dependencies": {
        "aos": "^3.0.0-beta.6",
        "core-js": "^3.3.2",
        "daterangepicker": "^3.0.5",
        "jquery.scrollto": "^2.1.2",
        "mdbvue": "./mdb/mdbvue/mdbvue-v6.0.1.tgz",
        "moment": "^2.24.0",
        "twilio-client": "^1.10.0",
        "typeface-poppins": "0.0.72",
        "vue-phone-number-input": "^1.1.6",
        "vue-progressbar": "^0.7.5",
        "vue-select": "^3.1.0",
        "vue-tables-2": "^1.4.70"
    },
    "devDependencies": {
        "@vue/cli-service": "^4.0.5",
        "axios": "^0.18",
        "cross-env": "^5.1",
        "jquery": "^3.4.0",
        "laravel-mix": "^5.0.4",
        "lodash": "^4.17.5",
        "node-sass": "^4.13.1",
        "popper.js": "^1.12",
        "resolve-url-loader": "^2.3.1",
        "sass": "^1.15.2",
        "sass-loader": "^8.0.0",
        "vue": "^2.6.10",
        "vue-cli-plugin-mdb": "^2.0.2",
        "vue-loader": "^15.7.0",
        "vue-template-compiler": "^2.6.10"
    },
    "engines": {
        "node": "12.13.1",
        "npm": "6.12.1"
    }
}
