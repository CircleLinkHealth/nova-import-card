{
    "private": true,
    "scripts": {
        "build-cpm": "COMPOSER_MEMORY_LIMIT=-1 composer install && php artisan migrate && php artisan migrate:views && npm install",
        "build-dev": "npm run build-cpm && npm run dev && COMPOSER_MEMORY_LIMIT=-1 composer ide",
        "build-prod": "npm run build-cpm && npm run prod",
        "dev": "npm run development",
        "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch": "npm run development -- --watch",
        "watch-poll": "npm run watch -- --watch-poll",
        "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --disable-host-check --config=node_modules/laravel-mix/setup/webpack.config.js",
        "prod": "npm run production",
        "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "build-import-practice-staff-csv": "cd nova-components/ImportPracticeStaffCsv && npm run dev",
        "build-import-practice-staff-csv-prod": "cd nova-components/ImportPracticeStaffCsv  && npm install && npm run prod",
        "build-generate-patient-call-data-csv": "cd nova-components/GeneratePatientCallDataCsv && npm run dev",
        "build-generate-patient-call-data-csv-prod": "cd nova-components/GeneratePatientCallDataCsv && npm install && npm run prod",
        "build-clhImportCardExtended": "cd nova-components/ClhImportCardExtended && npm run dev",
        "build-clhImportCardExtended-prod": "cd nova-components/ClhImportCardExtended && npm install && npm run prod",
        "build-generate-success-stories-report": "cd nova-components/GenerateSuccessStoriesReport && npm run dev",
        "build-generate-success-stories-report-prod": "cd nova-components/GenerateSuccessStoriesReport && npm install && npm run prod",
        "build-enrollment-invites": "cd nova-components/EnrollmentInvites && npm run dev",
        "build-enrollment-invites-prod": "cd nova-components/EnrollmentInvites && npm install && npm run prod",
        "build-invoices-download": "cd nova-components/InvoicesDownload && npm run dev",
        "build-invoices-download-prod": "cd nova-components/InvoicesDownload && npm install && npm run prod",
        "build-cpm-admin": "cd CircleLinkHealth/CpmAdmin && npm install && npm run dev",
        "build-cpm-admin-prod": "cd CircleLinkHealth/CpmAdmin && npm install && npm run prod"
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^1.2.30",
        "@fortawesome/free-solid-svg-icons": "^5.14.0",
        "@fortawesome/vue-fontawesome": "^0.1.10",
        "@fullcalendar/core": "^4.4.2",
        "bootstrap": "^3.3.7",
        "es6-string-polyfills": "^1.0.0",
        "jquery": "^3.5.1",
        "laravel-echo": "^1.8.0",
        "logdna": "^3.5.1",
        "materialize-css": "^0.99.0",
        "moment": "^2.27.0",
        "popper.js": "^1.16.1",
        "pusher-js": "^4.4.0",
        "rrule": "^2.6.4",
        "select2": "^4.0.13",
        "velocity-animate": "^1.5.0",
        "vue-axios": "^2.0.2",
        "vue-form": "^4.4.1",
        "vue-full-calendar": "^2.8.0",
        "vue-multiselect": "^2.1.6",
        "vue-select": "^2.5.1",
        "vue-simple-suggest": "^1.10.1",
        "vue-tables-2": "^0.6.99",
        "vuejs-datepicker": "^1.6.2",
        "vuex": "^2.4.0"
    },
    "devDependencies": {
        "@sentry/webpack-plugin": "^1.12.1",
        "axios": "^0.19",
        "cross-env": "^7.0",
        "laravel-mix": "^5.0.1",
        "laravel-mix-merge-manifest": "^0.1.2",
        "lodash": "^4.17.19",
        "resolve-url-loader": "^3.1.0",
        "sass": "^1.15.2",
        "sass-loader": "^8.0.0",
        "vue-template-compiler": "^2.6.12"
    }
}
