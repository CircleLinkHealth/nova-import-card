{
  "private": true,
  "scripts": {
    "dev": "bower install && npm run development",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch-poll": "npm run watch -- --watch-poll",
    "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "bower install && npm run production",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "test": "jest --verbose --coverage",
    "watch-test": "npm run test -- --watch"
  },
  "jest": {
    "verbose": true,
    "moduleNameMapper": {
      "^vue$": "vue/dist/vue.common.js"
    },
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/jest-vue-preprocessor"
    },
    "setupFiles": [
      "<rootDir>/resources/assets/js/tests/index.js"
    ]
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "dependencies": {
    "autocomplete-vue": "^1.0.3",
    "axios": "^0.16.2",
    "bootstrap-sass": "^3.3.7",
    "bower": "^1.8.4",
    "cross-env": "^5.1.6",
    "es6-string-polyfills": "^1.0.0",
    "jquery": "^3.3.1",
    "laravel-mix": "^2.1.11",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "lodash": "^4.17.10",
    "material-design-lite": "^1.3.0",
    "materialize-css": "^0.99.0",
    "moment": "^2.22.2",
    "node-excel-export": "github:mykeels/node-excel-export#development",
    "select2": "^4.0.5",
    "v-autocomplete": "^1.8.2",
    "v-complete": "github:mykeels/v-complete",
    "velocity-animate": "^1.5.1",
    "vue": "^2.5.16",
    "vue-axios": "^2.1.1",
    "vue-form": "^4.9.0",
    "vue-mdl": "^1.1.1",
    "vue-select": "github:sagalbot/vue-select",
    "vue-tables-2": "^0.6.99",
    "vue2-dropzone": "^2.3.6",
    "vuejs-datepicker": "github:mykeels/vuejs-datepicker#dist",
    "vuejs-paginator": "^2.0.2",
    "vuex": "^2.5.0",
    "webpack": "3.5.5"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.15.0",
    "babel-jest": "^21.2.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-es2015": "^6.24.1",
    "jest": "^21.2.1",
    "jest-vue-preprocessor": "^1.4.0",
    "mock-local-storage": "^1.0.5",
    "vue-test-utils": "^1.0.0-beta.11",
    "workbox-webpack-plugin": "^2.1.3",
    "xlsx": "^0.11.19",
    "xlsx-style": "^0.8.13"
  }
}
