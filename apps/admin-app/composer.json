    "config": {
        "discard-changes": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "github-oauth": {
            "github.com": "e0e31db02693e426dc2be482ecbee4daeafd8196"
        },
        "process-timeout": 3000
    },
    "extra": {
        "module-dir": "CircleLinkHealth",
        "laravel": {
            "dont-discover": [
                "barryvdh/laravel-snappy"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "CircleLinkHealth\\": "CircleLinkHealth/"
        },
        "classmap": [
            "database/seeders",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan nova:publish",
            "@php artisan vapor-ui:publish --ansi"
        ],
        "ide": [
            "php artisan ide-helper:generate",
            "php artisan -vvv ide-helper:models -W",
            "php artisan ide-helper:meta",
            "php -d memory_limit=2048M ./vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix --using-cache=no --show-progress=dots --diff --verbose --config=.php_cs.dist",
            "php -d memory_limit=2048M ./vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix --using-cache=no --show-progress=dots --diff --verbose --config=.php_cs_views.php"
        ]
    }
