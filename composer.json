{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "git",
            "url": "git@github.com:jamisonvalenta/Laravel-4-Generators.git"
        },
        {
            "type": "git",
            "url": "git@github.com:michalisantoniou6/url-shortener.git"
        },
        {
            "type": "composer",
            "url": "https://packagist.org"
        },
        {
            "packagist": false
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "amnuts/opcache-gui": "^2.4",
        "appstract/laravel-opcache": "^2.0",
        "arcturial/clickatell": "*",
        "barryvdh/laravel-debugbar": "*",
        "barryvdh/laravel-snappy": "*",
        "fideloper/proxy": "~3.3",
        "gemorroj/archive7z": "^3.0",
        "genealabs/laravel-caffeine": "^0.5.0",
        "guzzlehttp/guzzle": "~5.3|~6.0",
        "h4cc/wkhtmltoimage-amd64": "*",
        "h4cc/wkhtmltopdf-amd64": "*",
        "jenssegers/agent": "^2.5",
        "jeremykenedy/slack-laravel": "^2.2",
        "lab404/laravel-impersonate": "^1.2",
        "laracasts/utilities": "~2.0",
        "laravel/framework": "5.5.*",
        "laravel/passport": "^4.0",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "5.5.*",
        "league/flysystem-aws-s3-v3": "~1.0",
        "league/fractal": "0.12.*",
        "lynx39/lara-pdf-merger": "^1.0",
        "maatwebsite/excel": "^2.1",
        "michalisantoniou6/laravel-cerberus": "dev-master",
        "nao-pon/flysystem-google-drive": "~1.1",
        "phaxio/phaxio": "^1.0",
        "php-pm/httpkernel-adapter": "1.*",
        "php-pm/php-pm": "1.*",
        "predis/predis": "^1.1",
        "prettus/l5-repository": "dev-master",
        "prettus/laravel-validation": "*",
        "propaganistas/laravel-phone": "^3.0",
        "rap2hpoutre/laravel-log-viewer": "^0.4.0",
        "signifly/laravel-database-refactors": "^1.0",
        "spatie/laravel-google-calendar": "^2.0",
        "spatie/laravel-medialibrary": "^6.9",
        "spiritix/lada-cache": "^2.1",
        "twilio/sdk": "5.7",
        "tylercd100/lern": "^3.8",
        "tymon/jwt-auth": "0.5.*",
        "venturecraft/revisionable": "1.*",
        "waavi/url-shortener": "dev-master",
        "yajra/laravel-datatables-oracle": "~6.0"
    },
    "require-dev": {
        "Xethron/Laravel-4-Generators": "*",
        "barryvdh/laravel-ide-helper": "^2.4",
        "darkaonline/l5-swagger": "^5.5",
        "doctrine/dbal": "~2.3",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "laravel/dusk": "^2.0",
        "mockery/mockery": "0.9.*",
        "orangehill/iseed": "dev-master",
        "phpspec/phpspec": "~2.1",
        "phpunit/phpunit": "~6.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*",
        "xethron/migrations-generator": "dev-l5",
        "brianium/paratest": "*"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories",
            "database/refactors"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]

    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta"
        ],
        "test": [
          "./vendor/bin/paratest ./tests/Unit --stop-on-failure --colors --runner SqliteRunner -p8"
        ],
        "integration": [
          "./vendor/bin/paratest ./tests/integration --stop-on-failure --colors --runner SqliteRunner"
        ],
        "feature": [
          "./vendor/bin/paratest ./tests/feature --stop-on-failure --colors --runner SqliteRunner"
        ],
        "clear": [
            "php artisan clear-compiled",
            "composer dump-autoload",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "github-oauth": {
            "github.com": "e0e31db02693e426dc2be482ecbee4daeafd8196"
        },
        "process-timeout": 0
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/dusk"
            ]
        }
    }
}
