{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "repositories": [
    {
      "type": "git",
      "url": "git@github.com:jamisonvalenta/Laravel-4-Generators.git"
    },
    {
      "type": "composer",
      "url": "https://packagist.org"
    },
    {
      "packagist": false
    }
  ],
  "require": {
    "php": ">=5.6.4",
    "arcturial/clickatell": "*",
    "barryvdh/laravel-debugbar": "*",
    "barryvdh/laravel-snappy": "^0.3.0",
    "chrisbjr/api-guard": "~2.0",
    "dionera/laravel-beanstalkd-ui": "^0.3.4",
    "dusterio/laravel-aws-worker": "^0.1.5",
    "fideloper/proxy": "~3.1",
    "genealabs/laravel-caffeine": "^0.4.2",
    "guzzlehttp/guzzle": "~5.3|~6.0",
    "h4cc/wkhtmltoimage-amd64": "0.12.x",
    "h4cc/wkhtmltopdf-amd64": "0.12.x",
    "jeremykenedy/slack-laravel": "^2.2",
    "laracasts/utilities": "~2.0",
    "laravel/framework": "5.4.*",
    "laravel/tinker": "^1.0",
    "laravelcollective/html": "~5.0",
    "league/fractal": "0.12.*",
    "maatwebsite/excel": "~2.1.0",
    "modelizer/selenium": "~1.0",
    "pda/pheanstalk": "~3.0",
    "phaxio/phaxio": "^1.0",
    "prettus/l5-repository": "dev-master",
    "prettus/laravel-validation": "*",
    "rap2hpoutre/laravel-log-viewer": "^0.4.0",
    "rigor789/airbrake-laravel": "^1.2",
    "spatie/laravel-google-calendar": "^1.0",
    "spatie/laravel-tinker-tools": "^1.0",
    "twilio/sdk": "5.7",
    "tylercd100/lern": "^3.8",
    "tymon/jwt-auth": "0.5.*",
    "venturecraft/revisionable": "1.*",
    "yajra/laravel-datatables-oracle": "~6.0",
    "zizaco/entrust": "5.2.x-dev"
  },
  "require-dev": {
    "Xethron/Laravel-4-Generators": "*",
    "doctrine/dbal": "~2.3",
    "fzaninotto/faker": "~1.4",
    "laravel/browser-kit-testing": "1.*",
    "mockery/mockery": "0.9.*",
    "orangehill/iseed": "dev-master",
    "phpspec/phpspec": "~2.1",
    "phpunit/phpunit": "~5.7",
    "phpunit/phpunit-selenium": "^3.0",
    "symfony/css-selector": "3.1.*",
    "symfony/dom-crawler": "3.1.*",
    "xethron/migrations-generator": "dev-l5"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "app/helpers.php"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php",
      "tests/BrowserKitTestCase.php"
    ],
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ],
    "post-install-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postInstall",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate",
      "php artisan optimize"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true,
    "github-oauth": {
      "github.com": "e0e31db02693e426dc2be482ecbee4daeafd8196"
    },
    "process-timeout": 0
  }
}
