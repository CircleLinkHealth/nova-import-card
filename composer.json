{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "repositories": [
        {
            "type": "git",
            "url": "git@github.com:CircleLinkHealth/module-core.git"
        },
        {
            "type": "git",
            "url": "git@github.com:CircleLinkHealth/module-customer.git"
        },
        {
            "type": "git",
            "url": "git@github.com:CircleLinkHealth/shared-models-module.git"
        },
        {
            "type": "git",
            "url": "git@github.com:circlelinkhealth/revisionable-module.git"
        },
        {
            "type": "git",
            "url": "git@github.com:CircleLinkHealth/cpm-migrations-module.git"
        },
        {
            "type": "git",
            "url": "git@github.com:CircleLinkHealth/ccm-billing-module.git"
        },
        {
            "type": "git",
            "url": "git@github.com:CircleLinkHealth/module-two-fa.git"
        },
        {
            "type": "git",
            "url": "git@github.com:CircleLinkHealth/eligibility-module.git"
        },
        {
            "type": "git",
            "url": "git@github.com:CircleLinkHealth/synonyms.git"
        },
        {
            "type": "git",
            "url": "https://github.com/CircleLinkHealth/laravel-module-installer.git"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.2.0",
        "nwidart/laravel-modules": "dev-master",
        "ext-json": "*",
        "ext-simplexml": "*",
        "ext-redis": "*",
        "circlelinkhealth/core-module": "dev-vaporizing-admin",
        "circlelinkhealth/customer-module": "dev-vaporizing-admin",
        "circlelinkhealth/revisionable-module": "6.1.1",
        "circlelinkhealth/shared-models-module": "dev-vaporizing-admin",
        "joshbrw/laravel-module-installer": "^0.1.5",
        "circlelinkhealth/cpm-migrations-module": "dev-vaporizing-admin",
        "propaganistas/laravel-phone": "^4.1",
        "sentry/sentry-laravel": "1.7.0",
        "twilio/sdk": "5.31.3",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^6.3.1|^7.0.1",
        "laravel/framework": "^7.29",
        "laravel/tinker": "^2.5"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "*",
        "barryvdh/laravel-ide-helper": "^2.6",
        "friendsofphp/php-cs-fixer": "^2.16.3",
        "facade/ignition": "^2.0",
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.3",
        "phpunit/phpunit": "^8.5.8|^9.3.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "github-oauth": {
            "github.com": "e0e31db02693e426dc2be482ecbee4daeafd8196"
        }
    },
    "extra": {
        "module-dir": "CircleLinkHealth",
        "laravel": {
            "dont-discover": [
                "barryvdh/laravel-snappy"
            ]
        }
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "CircleLinkHealth\\": "CircleLinkHealth/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "lint-php": [
            "php -d memory_limit=2048M ./vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix --show-progress=dots --diff --verbose --config=.php_cs.dist"
        ],
        "lint-views": [
            "php -d memory_limit=2048M ./vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix --show-progress=dots --diff --verbose --config=.php_cs_views.php"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:models -W --dir=\"app\""
        ],
        "ide": [
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:models -W --dir=\"app\""
        ]
    }
}
