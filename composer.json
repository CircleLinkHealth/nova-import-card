{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/tralves/Swaggervel"
    },
    {
      "type": "vcs",
      "url": "https://github.com/michalisantoniou6/php-ccd-parser"
    }
  ],
  "require": {
    "laravel/framework": "5.0.*",
    "franzose/closure-table": "4.*",
    "chrisbjr/api-guard": "~2.0",
    "tymon/jwt-auth": "0.5.*",
    "barryvdh/laravel-cors": "0.7.x",
    "laravelcollective/html": "~5.0",
    "guzzlehttp/guzzle": "~5.3|~6.0",
    "rap2hpoutre/laravel-log-viewer": "^0.4.0",
    "jlapp/swaggervel": "master-dev",
    "zizaco/entrust": "dev-laravel-5#be28aa634c44551ae5187894c903094a8ce8baee",
    "fideloper/proxy": "~3.0",
    "mikemclin/laravel-wp-password": "~2.0.0",
    "michalisantoniou6/php-ccd-parser": "dev-master",
    "barryvdh/laravel-debugbar": "^2.0",
    "laracasts/utilities": "~2.0",
    "venturecraft/revisionable": "1.*",
    "rigor789/airbrake-laravel": "^1.2",
    "arcturial/clickatell": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "phpspec/phpspec": "~2.1",
    "doctrine/dbal": "~2.3"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "scripts": {
    "post-install-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-create-project-cmd": [
      "php -r \"copy('.env.example', '.env');\"",
      "php artisan key:generate"
    ]
  },
  "config": {
    "preferred-install": "dist"
  }
}
