{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "repositories": [
    {
      "type": "git",
      "url": "git@github.com:jamisonvalenta/Laravel-4-Generators.git"
    },
    {
      "type": "git",
      "url": "git@github.com:michalisantoniou6/url-shortener.git"
    },
    {
      "type": "git",
      "url": "git@github.com:michalisantoniou6/revisionable.git"
    },
    {
      "type": "git",
      "url": "git@github.com:CircleLinkHealth/opcache-gui.git"
    },
    {
      "type": "git",
      "url": "git@github.com:ishansiddiqui/lara-pdf-merger.git"
    },
    {
      "type": "git",
      "url": "git@github.com:CircleLinkHealth/laravel-cerberus.git"
    },
    {
      "type": "composer",
      "url": "https://packagist.org"
    },
    {
      "packagist": false
    }
  ],
  "require": {
    "php": ">=7.2.0",
    "ext-dom": "*",
    "ext-json": "*",
    "ext-libxml": "*",
    "amnuts/opcache-gui": "dev-master",
    "appstract/laravel-opcache": "^2.0",
    "arcturial/clickatell": "*",
    "authy/php": "^3.0",
    "barryvdh/laravel-debugbar": "*",
    "barryvdh/laravel-snappy": "0.4.1",
    "blackfire/php-sdk": "^1.17",
    "fideloper/proxy": "~3.3",
    "genealabs/laravel-caffeine": "^0.5.0",
    "guzzlehttp/guzzle": "~5.3|~6.0",
    "jenssegers/agent": "^2.5",
    "jeremykenedy/slack-laravel": "^2.2",
    "lab404/laravel-impersonate": "^1.2",
    "laracasts/utilities": "~2.0",
    "laravel/framework": "5.5.*",
    "laravel/horizon": "^1.4",
    "laravel/passport": "^4.0",
    "laravel/tinker": "^1.0",
    "laravelcollective/html": "5.5.*",
    "league/flysystem-aws-s3-v3": "~1.0",
    "league/fractal": "0.12.*",
    "lynx39/lara-pdf-merger": "dev-php7.2-adjustments",
    "maatwebsite/excel": "^2.1",
    "michalisantoniou6/laravel-cerberus": "dev-master",
    "nao-pon/flysystem-google-drive": "~1.1",
    "phaxio/phaxio": "^1.0",
    "pragmarx/health": "^0.9.16",
    "predis/predis": "^1.1",
    "prettus/l5-repository": "dev-master",
    "prettus/laravel-validation": "*",
    "propaganistas/laravel-phone": "^3.0",
    "rap2hpoutre/laravel-log-viewer": "^0.4.0",
    "seld/jsonlint": "^1.7",
    "spatie/laravel-backup": "^5.10",
    "spatie/laravel-google-calendar": "^2.0",
    "spatie/laravel-medialibrary": "^7.0.0",
    "spiritix/lada-cache": "^2.1",
    "squizlabs/php_codesniffer": "*",
    "twilio/sdk": "5.7",
    "tylercd100/lern": "^3.8",
    "tymon/jwt-auth": "0.5.*",
    "venturecraft/revisionable": "dev-master",
    "waavi/url-shortener": "dev-master",
    "yajra/laravel-datatables-oracle": "~8.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.13",
    "Xethron/Laravel-4-Generators": "*",
    "barryvdh/laravel-ide-helper": "^2.4",
    "darkaonline/l5-swagger": "^5.5",
    "doctrine/dbal": "~2.3",
    "filp/whoops": "~2.0",
    "fzaninotto/faker": "~1.4",
    "laravel/dusk": "^2.0",
    "mockery/mockery": "0.9.*",
    "orangehill/iseed": "dev-master",
    "phpspec/phpspec": "~2.1",
    "phpunit/phpunit": "~6.0",
    "symfony/css-selector": "3.1.*",
    "symfony/dom-crawler": "3.1.*",
    "xethron/migrations-generator": "dev-l5",
    "brianium/paratest": "*"
  },
  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "app/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "php-lint": [
      "php -d memory_limit=2048M ./vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix --show-progress=dots --diff -vvv"
    ],
    "views-lint": [
      "php -d memory_limit=2048M ./vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix resources --show-progress=dots --diff --rules=-fully_qualified_strict_types,-no_unused_imports -vvv"
    ],
    "post-root-package-install": [
      "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate"
    ],
    "test-db-mysql": [
      "php artisan config:clear && php artisan db:test && php artisan migrate:fresh && php artisan migrate && php artisan db:seed && php artisan db:seed --class=TestSuiteSeeder"
    ],
    "test-mysql": [
      "./vendor/bin/paratest ./tests/Unit --stop-on-failure --colors --runner WrapperRunner -p8"
    ],
    "test": [
      "./vendor/bin/paratest ./tests/Unit --stop-on-failure --colors --runner SqliteRunner -p8"
    ],
    "integration-sqlite": [
      "./vendor/bin/paratest ./tests/integration --stop-on-failure --colors --runner SqliteRunner"
    ],
    "feature-sqlite": [
      "./vendor/bin/paratest ./tests/feature --stop-on-failure --colors --runner SqliteRunner"
    ],
    "clear": [
      "php artisan clear-compiled",
      "composer dump-autoload",
      "php artisan optimize"
    ]
  },
  "config": {
    "discard-changes": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true,
    "github-oauth": {
      "github.com": "e0e31db02693e426dc2be482ecbee4daeafd8196"
    },
    "process-timeout": 0
  },
  "extra": {
    "laravel": {
      "dont-discover": [
        "laravel/dusk"
      ]
    }
  }
}
