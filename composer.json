{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/tralves/Swaggervel"
    },
    {
      "type": "vcs",
      "url": "https://github.com/michalisantoniou6/php-ccd-parser"
    }
  ],
  "require": {
    "laravel/framework": "5.2.*",
    "franzose/closure-table": "4.*",
    "chrisbjr/api-guard": "~2.0",
    "tymon/jwt-auth": "0.5.*",
    "barryvdh/laravel-cors": "0.7.x",
    "laravelcollective/html": "~5.0",
    "guzzlehttp/guzzle": "~5.3|~6.0",
    "rap2hpoutre/laravel-log-viewer": "^0.4.0",
    "zizaco/entrust": "5.2.x-dev",
    "fideloper/proxy": "~3.1",
    "michalisantoniou6/php-ccd-parser": "dev-master",
    "barryvdh/laravel-debugbar": "^2.0",
    "laracasts/utilities": "~2.0",
    "venturecraft/revisionable": "1.*",
    "rigor789/airbrake-laravel": "^1.2",
    "arcturial/clickatell": "*",
    "barryvdh/laravel-snappy": "^0.3.0",
    "h4cc/wkhtmltopdf-amd64": "0.12.x",
    "h4cc/wkhtmltoimage-amd64": "0.12.x",
    "maatwebsite/excel": "~2.1.0",
    "prettus/l5-repository": "^2.1",
    "laravelcollective/bus": "5.2.x",
    "maknz/slack": "^1.7",
    "dusterio/laravel-aws-worker": "^0.1.5"
  },
  "require-dev": {
    "symfony/dom-crawler": "~3.0",
    "symfony/css-selector": "~3.0",
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~4.0",
    "phpspec/phpspec": "~2.1",
    "doctrine/dbal": "~2.3",
    "orangehill/iseed": "dev-master"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "App\\": "app/",
      "Tests\\": "tests/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "scripts": {
    "post-install-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-create-project-cmd": [
      "php -r \"copy('.env.example', '.env');\"",
      "php artisan key:generate"
    ]
  },
  "config": {
    "preferred-install": "dist"
  }
}
