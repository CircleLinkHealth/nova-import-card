{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "keywords": [
        "framework",
        "laravel",
        "lumen"
    ],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "git",
            "url": "git@github.com:CircleLinkHealth/module-core.git"
        },
        {
            "type": "git",
            "url": "git@github.com:CircleLinkHealth/module-customer.git"
        },
        {
            "type": "git",
            "url": "git@github.com:CircleLinkHealth/shared-models-module.git"
        },
        {
            "type": "git",
            "url": "git@github.com:circlelinkhealth/revisionable-module.git"
        },
        {
            "type": "git",
            "url": "git@github.com:CircleLinkHealth/cpm-migrations-module.git"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "ext-json": "*",
        "ext-simplexml": "*",
        "ext-redis": "*",
        "barryvdh/laravel-cors": "^0.11.3",
        "barryvdh/laravel-ide-helper": "^2.6",
        "circlelinkhealth/core-module": "1.5.5",
        "circlelinkhealth/customer-module": "1.9.3",
        "circlelinkhealth/revisionable-module": "^6.1",
        "circlelinkhealth/shared-models-module": "1.5.6",
        "circlelinkhealth/cpm-migrations-module": "dev-ROAD-176_twilio_calls_source",
        "illuminate/redis": "^6",
        "illuminate/session": "^6",
        "laravel/lumen-framework": "^6",
        "laravel/tinker": "^1.0",
        "predis/predis": "^1.1",
        "propaganistas/laravel-phone": "^4.1",
        "sentry/sentry-laravel": "1.7.0",
        "twilio/sdk": "5.31.3",
        "vlucas/phpdotenv": "^3"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.4",
        "phpunit/phpunit": "^7.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:models -W --dir=\"app\""
        ],
        "ide": [
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:models -W --dir=\"app\""
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "github-oauth": {
            "github.com": "e0e31db02693e426dc2be482ecbee4daeafd8196"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
