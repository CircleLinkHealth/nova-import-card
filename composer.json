  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "App\\": "app/",
      "Modules\\": "Modules/"
    },
    "files": [
      "app/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    },
    "files": [
      "Modules/Core/Helpers/helpers.php"
    ]
  },
  "scripts": {
    "lint-php": [
      "php -d memory_limit=2048M ./vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix --show-progress=dots --diff --verbose --config=.php_cs.dist"
    ],
    "lint-views": [
      "php -d memory_limit=2048M ./vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix --show-progress=dots --diff --verbose --config=.php_cs_views.php"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate",
      "@php artisan nova:publish"
    ],
    "unit-tests": [
      "php artisan --env=testing test:prepare-test_suite-db",
      "APP_ENV=testing ./vendor/phpunit/phpunit/phpunit --testsuite=Unit --colors"
    ],
    "feature-tests": [
      "php artisan --env=testing test:prepare-test_suite-db",
      "APP_ENV=testing ./vendor/phpunit/phpunit/phpunit --testsuite=Feature --colors"
    ],
    "test": [
      "php artisan --env=testing test:prepare-test_suite-db",
      "php artisan --env=testing migrate:fresh",
      "php artisan --env=testing migrate:views",
      "php artisan --env=testing db:seed --class=TestSuiteSeeder",
      "./vendor/phpunit/phpunit/phpunit --testsuite=NurseInvoicesModule --colors --testdox",
      "./vendor/phpunit/phpunit/phpunit --testsuite=EligibilityModule --colors --testdox",
      "./vendor/phpunit/phpunit/phpunit --testsuite=Feature --colors --testdox",
      "./vendor/phpunit/phpunit/phpunit --testsuite=Unit --colors --testdox"
    ],
    "clear": [
      "php artisan clear-compiled",
      "composer dump-autoload"
    ],
    "ide": [
      "php artisan ide-helper:generate",
      "php artisan -vvv ide-helper:models -W",
      "php artisan ide-helper:meta",
      "php -d memory_limit=2048M ./vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix --using-cache=no --show-progress=dots --diff --verbose --config=.php_cs.dist",
      "php -d memory_limit=2048M ./vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix --using-cache=no --show-progress=dots --diff --verbose --config=.php_cs_views.php"
    ]
  },
  "config": {
    "discard-changes": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true,
    "github-oauth": {
      "github.com": "e0e31db02693e426dc2be482ecbee4daeafd8196"
    },
    "process-timeout": 3000
  },
  "extra": {
    "laravel": {
      "dont-discover": [
        "laravel/dusk",
        "barryvdh/laravel-snappy"
      ]
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev"
}
