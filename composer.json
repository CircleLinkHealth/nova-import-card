{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "keywords": [
        "framework",
        "laravel",
        "lumen"
    ],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "git",
            "url": "git@github.com:CircleLinkHealth/module-core.git"
        },
        {
            "type": "git",
            "url": "git@github.com:CircleLinkHealth/module-customer.git"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "ext-simplexml": "*",
        "barryvdh/laravel-cors": "^0.11.3",
        "barryvdh/laravel-ide-helper": "^2.6",
        "circlelinkhealth/core": "dev-master",
        "circlelinkhealth/customer": "dev-master",
        "laravel/lumen-framework": "5.8.*",
        "laravel/tinker": "^1.0",
        "propaganistas/laravel-phone": "^4.1",
        "twilio/sdk": "^5.31",
        "vlucas/phpdotenv": "^3.3"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.4",
        "phpunit/phpunit": "^7.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
