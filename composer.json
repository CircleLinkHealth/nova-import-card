    "require-dev": {
        "ext-xdebug": "*",
        "barryvdh/laravel-ide-helper": "^2.6",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0",
        "facade/ignition": "^1.4"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "github-oauth": {
            "github.com": "e0e31db02693e426dc2be482ecbee4daeafd8196"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "circlelinkhealth/eligibility-module"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [
            "Modules/Core/Helpers/helpers.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": [
            "php artisan --env=testing test:prepare-test_suite-db",
            "php artisan --env=testing migrate:fresh",
            "php artisan --env=testing migrate:views",
            "php artisan --env=testing db:seed --class=TestSuiteSeeder",
            "./vendor/phpunit/phpunit/phpunit --testsuite=Feature,Unit --colors --testdox"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "clear": [
            "php artisan clear-compiled",
            "composer dump-autoload"
        ],
        "ide": [
            "php artisan ide-helper:generate",
            "php artisan -vvv ide-helper:models --dir='app/' --dir='Modules/'",
            "php artisan ide-helper:meta",
            "php -d memory_limit=2048M ./vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix --using-cache=no --show-progress=dots --diff --verbose --config=.php_cs.dist",
            "php -d memory_limit=2048M ./vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix --using-cache=no --show-progress=dots --diff --verbose --config=.php_cs_views.php"
        ],
        "lint-php": [
            "php -d memory_limit=2048M ./vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix --show-progress=dots --diff --verbose --config=.php_cs.dist"
        ],
        "lint-views": [
            "php -d memory_limit=2048M ./vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix --show-progress=dots --diff --verbose --config=.php_cs_views.php"
        ]
    }
}
