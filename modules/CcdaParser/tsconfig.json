{
  "compilerOptions": {
    "target": "es6",
    "lib": [
      "esnext"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "typeRoots": [
      "./typings",
      "./node_modules/@types"
    ],
    "noImplicitAny": true,
    "removeComments": false,
    "preserveConstEnums": true,
    "sourceMap": true,
    "types": [
      "node"
    ]
  }
}
