{
    "name": "circlelinkhealth/pdf-service-module",
    "type": "laravel-module",
    "description": "",
    "authors": [
        {
            "name": "Pangratios Cosma",
            "email": "pangratios@cirlelinkhealth.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "CircleLinkHealth\\PdfService\\Providers\\PdfServiceProvider",
                "CircleLinkHealth\\PdfService\\Providers\\PdfServiceDeferrableProvider"
            ],
            "aliases": {

            },
            "dont-discover": [
                "barryvdh/laravel-snappy"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "CircleLinkHealth\\PdfService\\": ""
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../Core"
        },
        {
            "type": "vcs",
            "url": "https://github.com/api2pdf/api2pdf.php"
        }
    ],
    "require": {
        "circlelinkhealth/core-module": "*",
        "daltcore/lara-pdf-merger": "2.0.6"
    },
    "suggest": {
        "api2pdf/api2pdf.php": "Required to use Api2Pdf",
        "barryvdh/laravel-snappy": "Required to use SnappyPdf (wkhtmltopdf)"
    }
}
